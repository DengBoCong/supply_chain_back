<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.power.dbc.Model.CommodityClassEntity" table="commodity_class" schema="supply_chain">
        <id name="number">
            <column name="number" sql-type="varchar(20)" length="20" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="alais">
            <column name="alais" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="owner">
            <column name="owner" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>