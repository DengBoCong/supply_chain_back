<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.power.dbc.Model.CommodityEntity" table="commodity" schema="supply_chain">
        <id name="number">
            <column name="number" sql-type="varchar(100)" length="100" not-null="true"/>
        </id>
        <property name="image">
            <column name="image" sql-type="varchar(1000)" length="1000"/>
        </property>
        <property name="classification">
            <column name="classification" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="unit">
            <column name="unit" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="price">
            <column name="price" sql-type="double" precision="-1"/>
        </property>
        <property name="alias">
            <column name="alias" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="procurementType">
            <column name="procurementType" sql-type="int(11)"/>
        </property>
        <property name="purchaser">
            <column name="purchaser" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int(11)"/>
        </property>
        <property name="stock">
            <column name="stock" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>